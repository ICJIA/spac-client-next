{
  "name": "spac-client-next-prod",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "node buildSearchIndex.js && node buildSitemap.js && npm run docs:generate && vue-cli-service serve",
    "build": "node buildSearchIndex.js && node buildSitemap.js && npm run docs:generate && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:search": "node buildSearchIndex.js",
    "create:documentation": "node ./creators/createDocumentationHTML.js",
    "docs:dev": "node ./creators/createDocumentationHTML.js",
    "docs:jsdoc": "jsdoc -c jsdoc.conf.json",
    "docs:generate": "npm run docs:dev && npm run docs:jsdoc"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.0.0-rc.12",
    "aos": "^3.0.0-beta.6",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.4.8",
    "crypto-js": "^3.1.9-1",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "fuse.js": "^3.4.6",
    "graphql-request": "^1.8.2",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^3.0.1",
    "markdown-it-named-headers": "0.0.4",
    "moment": "^2.24.0",
    "netlify-lambda": "^1.6.3",
    "nprogress": "^0.2.0",
    "rimraf": "^3.0.0",
    "sitemap": "^5.1.0",
    "sitemap-generator": "^8.4.2",
    "thumbor-url-builder": "^0.1.1",
    "vue": "^2.6.12",
    "vue-analytics": "^5.22.1",
    "vue-browser-detect-plugin": "^0.1.13",
    "vue-content-loader": "^0.2.3",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.4.3",
    "vue-router-back-button": "^1.3.0",
    "vuetify": "^2.4.3",
    "vuex": "^3.5.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@mdi/font": "^5.6.55",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "clean-jsdoc-theme": "^4.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "node-sass": "^4.9.0",
    "prettier": "^1.19.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-moment": "^0.1.1",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.7.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "prefer-const": "warn",
      "no-var": "warn",
      "spaced-comment": [
        "warn",
        "always",
        {
          "line": {
            "markers": [
              "/"
            ],
            "exceptions": [
              "-",
              "+"
            ]
          },
          "block": {
            "markers": [
              "*"
            ],
            "exceptions": [
              "*"
            ],
            "balanced": true
          }
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
